<form method="post" id="church_info_form">
    <table class="t-edit">
		<tbody>
    <tr>
        <td width="124"><label>Date of baptism</label></td>
        <td>
            <span class="niceform">
                <select class="bp_month" id="bmonth" name="fm[bmonth]" size="1" style="width:61px;">
                    <option value="0">Month</option>
                    {foreach key=k item=i from=$mm}
                    <option value="{$k}"{if $k==$fm.bmonth} selected="selected"{/if}>{$i}</option>
	        {/foreach}
                </select>
            </span>
            <span class="niceform" style="float:left">
                <select class="bp_day" id="bday" name="fm[bday]" size="1" style="width:51px;">
                    <option value="0">Day</option>
	            {foreach key=k item=i from=$dd}
                    <option value="{$i}"{if $i==$fm.bday} selected="selected"{/if}>{$i}</option>
	            {/foreach}
                </select>
            </span>
            <span class="niceform" style="float:left">
                <select class="bp_year" id="byear" name="fm[byear]" size="1" style="width:55px;">
                    <option value="0">Year</option>
	            {foreach key=k item=i from=$yy}
                    <option value="{$i}"{if $i==$fm.byear} selected="selected"{/if}>{$i}</option>
	            {/foreach}
                </select>
            </span>
        </td>
    </tr></tbody>
    
    <tbody style="display:none;" id="church_mission_tmp">
    <tr>
        <td colspan="2"></td>
    </tr>
    </tbody>
    <!--for scripts-->
    <tbody id="church_mission_default" style="display:none">
		<tr>
			<td width="124"><label>Mission location</label></td>
			<td>
				<input type="hidden" id="mission_id" />
				<input type="hidden" name="fm[user_mission_id][]" value="0" />
				<input class="txt3 loc" type="text" name="fm[mission_location][]" value=""  />
				<div style="float:left;margin-top:4px;">
					<a  href="javascript:void(0);" onclick="oUsers.SettingsChurchDelMission( $(this).parent().parent().parent(), 0 );">Clear</a>
				</div>
			</td>
		</tr>
    <tr>
        <td width="124"><label>Mission time from</label></td>
        <td>
             <span class="niceform">
                <select cls="from_month" id="bmonth" name="fm[fmonth][]" size="1" style="width:61px;">
                    <option value="0">Month</option>
                    {foreach key=k item=i from=$mm}
                    <option value="{$k}">{$i}</option>
	        {/foreach}
                </select>
            </span>
            <span class="niceform" style="float:left">
                <select cls="from_day" id="bday" name="fm[fday][]" size="1" style="width:51px;">
                    <option value="0">Day</option>
	            {foreach key=k item=i from=$dd}
                    <option value="{$i}">{$i}</option>
	            {/foreach}
                </select>
            </span>
            <span class="niceform" style="float:left">
                <select cls="from_year" id="byear" name="fm[fyear][]" size="1" style="width:55px;">
                    <option value="0">Year</option>
	            {foreach key=k item=i from=$yy}
                    <option value="{$i}">{$i}</option>
	            {/foreach}
                </select>
            </span>
        </td>
    </tr>
    <tr>
        <td width="124"><label>to</label></td>
        <td>
            <span class="niceform">
                <select cls="to_month" id="bmonth" name="fm[tmonth][]" size="1" style="width:61px;">
                    <option value="0">Month</option>
                    {foreach key=k item=i from=$mm}
                    <option value="{$k}">{$i}</option>
	        {/foreach}
                </select>
            </span>
            <span class="niceform" style="float:left">
                <select cls="to_day" id="bday" name="fm[tday][]" size="1" style="width:51px;">
                    <option value="0">Day</option>
	            {foreach key=k item=i from=$dd}
                    <option value="{$i}">{$i}</option>
	            {/foreach}
                </select>
            </span>
            <span class="niceform" style="float:left">
                <select cls="to_year" id="byear" name="fm[tyear][]" size="1" style="width:55px;">
                    <option value="0">Year</option>
	            {foreach key=k item=i from=$yy}
                    <option value="{$i}">{$i}</option>
	            {/foreach}
                </select>
            </span>
        </td>
    </tr>
    </tbody>
    <!--for scripts-->
    <tbody id="church_mission">
    {foreach key=k1 item=i1 from=$mission}

	{*<tr><td width="124"><label>Mission location</label></td><td><input name="fm[mission_location][]" value="{$fm.mission_location.$k1}"  class="txt3 loc" type="text"  />*}
    
    <tr>
		<td width="124">
			<label>Mission location</label></td><td><input type="hidden" id="mission_id" /><input type="hidden" name="fm[user_mission_id][]" value="{if $fm.user_mission_id.$k1}{$fm.user_mission_id.$k1}{else}0{/if}" /><input class="txt3 loc" type="text" name="fm[mission_location][]" value="{$fm.mission_location.$k1}"  />
            <div style="float:left;margin-top:4px;"><a  href="javascript:void(0);" onclick="oUsers.SettingsChurchDelMission( $(this).parent().parent().parent(), {if $fm.user_mission_id.$k1}{$fm.user_mission_id.$k1}{else}0{/if} );">Clear</a></div>
	    </td>
	</tr>

