{elseif 'wards_wall' == $m_page}
{strip}
<!--  Ward's Messages LeftColumn  -->
<h2 class="lowprof">{$ward_i.title}{if $ward_i.ward_title},<br> {$ward_i.ward_title} {/if}</h2>
<ul class="list01">
	{if !$ward_i.im_member}<li><a href="javascript: void(0);" onclick="oWWall.SHConfirmPopup( 1, 'id_confirm_wards_popup', '{$ward_i.id}' );">Set as my ward</a></li>{/if}
	<li>{if !$wwhatch}<a id="id_wards_whatch" href="javascript: void(0);" onclick="oWards.EditWhatching( 1, '{$ward_i.id}' ) ;">Watch this ward</a>{else}<a id="id_wards_whatch" href="javascript: void(0);" onclick="oWards.EditWhatching( 2, '{$ward_i.id}' ) ;">Unwatch this ward</a>{/if}</li>
	<li><a href="{$siteAdr}wards/list">Choose other ward</a></li>
	{* <li><a href="#">Report incorrect info</a></li> *}
</ul>

{if $ar_whatching}
<h2>{if 5 < $cnt_whatching}<span><a href="{$siteAdr}wards/whatch">View all</a></span>{/if}Watching</h2>
<ul class="list01">
	{foreach name=n key=k item=i from=$ar_whatching}
    {if $smarty.foreach.n.index < 5}
	<li>
		<a href="{$siteAdr}wards/id{$i.wid}">{$i.ward_title}</a>
	</li>
    {/if}
	{/foreach}
</ul>
{/if}

<h2>Information</h2>
<ul class="list01">
	<li>
	{if $ward_i.country || $ward_i.city}
	    <span style="color: gray; font-size: 12px;">Location</span>
		<p>{$ward_i.city}, {if $ward_i.region}{$ward_i.region},{/if} {$ward_i.country}</p>
	{else}
	    There isn't any info
	{/if}
	</li>
</ul>


{if $ward_i.my_ward || $CAN_EDIT || ( $ward_i.bishopric.first_name || $ward_i.bishopric.last_name )}
<br /><br />
<h2>Bishopric</h2>
<ul class="list01">
	{if $ward_i.bishopric.first_name || $ward_i.bishopric.last_name}
	<li>
		<table>
			<tr>
				<td style="width: 25%;">
					<span style="color: gray;">Bishop</span>
				</td>
				<td style="width: 5%;">&nbsp</td>
				<td>&nbsp</td>
			</tr>
			<tr>
				<td align="center" style="{if $ward_i.bishopric.p_img} width:25%; {else} width:0px; {/if}">
					{assign var=prfname value=$ward_i.bishopric.first_name|cat:" "|cat:$ward_i.bishopric.last_name}
					<img src="{if $ward_i.bishopric.p_img} {$fImgDir}wards/info/bishopric/{$ward_i.bishopric.p_img} {else} {$imgDir}no_photo_m42.jpg {/if}" style="max-width: 42px; max-height: 42px;" title="{$prfname}" alt="{$prfname}" />
				</td>
				<td style="width: 5%;">&nbsp</td>
				<td>
					<table class="pres_table">
						<tr>
							<td>
								<span>{$prfname}</span>
							</td>
						</tr>
						<tr>
							<td>
								<span>{$ward_i.bishopric.phone}</span>
							</td>
						</tr>
						<tr>
							<td>
								<span>{$ward_i.bishopric.email}</span>
							</td>
						</tr>
					</table>
				</td>				
			</tr>
		</table>
	</li>
	{/if}
	
	{section name=j loop=$ar_addi}
	
		{assign var=str_first_name value=$ar_addi[j]|cat:"_first_name"}
		{assign var=str_last_name value=$ar_addi[j]|cat:"_last_name"}
		{assign var=str_phone value=$ar_addi[j]|cat:"_phone"}
		{assign var=str_email value=$ar_addi[j]|cat:"_email"}
		
		{if $ward_i.bishopric.$str_first_name || $ward_i.bishopric.$str_last_name}
		{assign var=prfname value=$ward_i.bishopric.$str_first_name|cat:" "|cat:$ward_i.bishopric.$str_last_name}
		<li>
			<table>
				
				<tr>
					<td style="width: 25%;">
						<span style="color: gray;">{if 'cp' == $ar_addi[j]}Contact Person{elseif 'fc' == $ar_addi[j]}First Counselor{elseif 'sc' == $ar_addi[j]}Second Counselor{elseif 'es' == $ar_addi[j]}Ward Executive Secretary{elseif 'wc' == $ar_addi[j]}Ward Clerk{/if}</span>
					</td>
				</tr>
				<tr>
					<td>
						<span>{$prfname}</span>
					</td>
				</tr>
				<tr>
					<td>
						<span>{$ward_i.bishopric.$str_phone}</span>
					</td>
				</tr>
				<tr>
					<td>
						<span>{$ward_i.bishopric.$str_email}</span>
					</td>
				</tr>
				
			</table>
		</li>
		{/if}
		
	{/section}
	
	{if $CAN_EDIT && $IS_USER && ($ward_i.my_ward || $ward_i.my_stake)}<li><a href="javascript: void(0);" onclick="oWards.SHBishopricPopup( 1, 'id_add_bishopric_popup' );">{if $ward_i.bishopric.first_name || $ward_i.bishopric.last_name} Edit bishopric info {else} Suggest bishopric info{/if}</a></li>{/if}
</ul>
{/if}
{/strip}
