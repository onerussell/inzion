		<tr><td style="padding-top:10px"><input name="ward_name" class="ward_name" type="text" value="Enter stake/ward name" onclick="this.value='';" /></td></tr>
		<tr>
			<td><div class="but-filter"><span><a class="cl_filt_btn2" style="cursor: pointer;">Filter</a></span></div></td>
		</tr>
	</table>
</div>











