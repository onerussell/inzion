function NFDo(what){var niceforms=$('span.niceform');var identifier=new RegExp('(^| )'+'niceform'+'( |$)');if(/MSIE (\d+\.\d+);/.test(navigator.userAgent)){var ieversion=new Number(RegExp.$1);if(ieversion<7){return false;}
