function Symb2Charset(symb)
{var arAlpha={'а':'a','б':'b','в':'v','г':'g','д':'d','е':'e','ё':'jo','ж':'zh','з':'z','и':'i','й':'j','к':'k','л':'l','м':'m','н':'n','о':'o','п':'p','р':'r','с':'s','т':'t','у':'u','ф':'f','х':'h','ц':'c','ч':'ch','ш':'sh','щ':'w','ъ':'#','ы':'y','ь':'\'','э':'je','ю':'ju','я':'ja','Б':'B','В':'V','Г':'G','Д':'D','Е':'E','Ё':'Jo','Ж':'Zh','З':'Z','И':'I','Й':'J','К':'K','Л':'L','М':'M','Н':'N','О':'O','П':'P','Р':'R','С':'S','Т':'T','У':'U','Ф':'F','Х':'H','Ц':'C','Ч':'Ch','Ш':'Sh','Щ':'W','Ъ':'##','Ы':'Y','Ь':'\'\'','Э':'Je','Ю':'Ju','Я':'Ja'};return arAlpha[symb]?arAlpha[symb]:symb;}
function Txt2Charse(txt){var arAlpha={'а':'a','б':'b','в':'v','г':'g','д':'d','е':'e','ё':'jo','ж':'zh','з':'z','и':'i','й':'j','к':'k','л':'l','м':'m','н':'n','о':'o','п':'p','р':'r','с':'s','т':'t','у':'u','ф':'f','х':'h','ц':'c','ч':'ch','ш':'sh','щ':'w','ъ':'#','ы':'y','ь':'\'','э':'je','ю':'ju','я':'ja','Б':'B','В':'V','Г':'G','Д':'D','Е':'E','Ё':'Jo','Ж':'Zh','З':'Z','И':'I','Й':'J','К':'K','Л':'L','М':'M','Н':'N','О':'O','П':'P','Р':'R','С':'S','Т':'T','У':'U','Ф':'F','Х':'H','Ц':'C','Ч':'Ch','Ш':'Sh','Щ':'W','Ъ':'##','Ы':'Y','Ь':'\'\'','Э':'Je','Ю':'Ju','Я':'Ja'};var ereg='';var txtvar=txt;for(var k in arAlpha)
{eval('ereg = /'+k+'/g;');txtvar=txtvar.replace(ereg,arAlpha[k]);}
return txtvar.toLowerCase();}
